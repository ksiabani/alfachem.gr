body
  padding 50px
  min-height 2000px
  padding-top 70px
  /*font 14px "Lucida Grande", Helvetica, Arial, sans-serif*/

a
  color #00B7FF

.navbar-fixed-top
  .nav
    padding 15px 0
  .navbar-brand
    padding 0 15px

@media (min-width: 768px)
  body
    padding-top 100px
  .navbar-fixed-top
    .navbar-brand
      padding 15px 0

@media (min-width: 979px)
  #sidebar
    .affix-top
      position static
      margin-top 30px
      width 228px
    .affix
      position fixed
      top 70px
      width 228px



